lf.tapped=False\n    self.callback=self.print_name\n    self.tap_x = -1\n    self.tap_y = -1\n\n    def is_tapped(self, x, y):\n        \"\"\"\n        label:\n            en: ' %1 is_tapped, x: %2, y: %3'\n        params:\n            x:\n                name: x\n            y:\n                name: y\n        \"\"\"\n    w=self.rect[2]\n    h=self.rect[3]\n    return (self.rect[0] < x and self.rect[0]+w > x and self.rect[1] < y and self.rect[1]+h > y)\n\n    def check(self):\n        \"\"\"\n        label:\n            en: ' %1 check'\n        \"\"\"\n    if self.callback:\n        if self.is_tapped(self.tap_x, self.tap_y):\n            if self.tapped:\n              try:\n                self.callback()\n              except:\n                print(\"Fail to execute callback\")\n              self.tap_x = -1\n              self.tap_y = -1\n    else:\n        print(\"Callback not fount\")\n    self.tapped = False\n    return\n\n    def check_tap(self):\n        \"\"\"\n        label:\n            en: ' %1 check_tap'\n        \"\"\"\n    self.tap_x = M5.Touch.getX()\n    self.tap_y = M5.Touch.getY()\n    if self.is_tapped(self.tap_x, self.tap_y):\n       self.tapped=True\n    return self.tapped\n\n    def set_callback(self, obj):\n        \"\"\"\n        label:\n            en: ' %1 set_callback, obj: %2'\n        params:\n            obj:\n                name: obj\n        \"\"\"\n    if isinstance(obj, str):\n      self.callback = eval(obj)\n    else:\n      try:\n        self.callback = obj.callback\n      except:\n        self.callback = None\n    return\n\n    def update(self):\n        \"\"\"\n        label:\n            en: ' %1 update'\n        \"\"\"\n    M5.update()\n    self.check()\n    return\n\n\n","version":"alpha2"}